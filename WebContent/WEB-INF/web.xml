<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Mommy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
   <servlet>
      <servlet-name>UserFrontController</servlet-name>
      <servlet-class>com.mommy.app.user.UserFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>UserFrontController</servlet-name>
      <url-pattern>*.user</url-pattern>
  </servlet-mapping>
  
     <servlet>
      <servlet-name>AskFrontController</servlet-name>
      <servlet-class>com.mommy.app.ask.AskFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>AskFrontController</servlet-name>
      <url-pattern>*.ask</url-pattern>
  </servlet-mapping>
  
     <servlet>
      <servlet-name>CommunityFrontController</servlet-name>
      <servlet-class>com.mommy.app.community.CommunityFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>CommunityFrontController</servlet-name>
      <url-pattern>*.com</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>NoticeFrontController</servlet-name>
      <servlet-class>com.mommy.app.notice.NoticeFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>NoticeFrontController</servlet-name>
      <url-pattern>*.no</url-pattern>
  </servlet-mapping>
  
   <servlet>
      <servlet-name>ServiceFrontController</servlet-name>
      <servlet-class>com.mommy.app.service.ServiceFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>ServiceFrontController</servlet-name>
      <url-pattern>*.ser</url-pattern>
  </servlet-mapping>
  
   <servlet>
      <servlet-name>MyPageFrontController</servlet-name>
      <servlet-class>com.mommy.app.myPage.MyPageFrontController</servlet-class>
   </servlet>
     <servlet-mapping>
      <servlet-name>MyPageFrontController</servlet-name>
      <url-pattern>*.my</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>AdminFrontController</servlet-name>
     <servlet-class>com.mommy.app.admin.AdminFrontController</servlet-class>
  </servlet>
  
   <servlet-mapping>
     <servlet-name>AdminFrontController</servlet-name>
     <url-pattern>*.ad</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ChatFrontController</servlet-name>
     <servlet-class>com.mommy.app.chat.ChatFrontController</servlet-class>
  </servlet>
  
   <servlet-mapping>
     <servlet-name>ChatFrontController</servlet-name>
     <url-pattern>*.chat</url-pattern>
  </servlet-mapping>
  
</web-app>