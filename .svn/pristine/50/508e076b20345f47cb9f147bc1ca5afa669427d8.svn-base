	background: url('../img/livecommerce_titleBG.png');
}
.livecommerce .title p{
	width: 335px;
	font-size: 14px;
	color: #fff;
	line-height: 1.7;
	margin: 20px auto;
	text-align: left;
}
.livecommerce .title .close{ /* dr 생성 210630 */
