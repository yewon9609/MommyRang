}
.livecommerce .title span{
	font-size: 11px;
	color: #fff;
	border-radius: 3px;
	background: #ec2014;
	padding: 2px 5px;
	margin-right: 6px;
}
.livecommerce .chat{
	position: absolute;
	bottom: 0;	
	width: 100%;
	height: 340px;	
	background: url('../img/livecommerce_chatBG.png');
