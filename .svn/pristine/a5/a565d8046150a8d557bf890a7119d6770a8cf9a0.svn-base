package com.mommy.app.ask.vo;

public class AskDTO {
	private int askNum;
	private int categoryNum;
	private int askStatus;
	private String askTitle;
	private String askContent;
	private String askAnswer;
	private String askDate;
	private int userNum;
	
	
	
	public AskDTO() {;}
	
	public AskDTO(AskVO askVO) {
		super();
		this.askNum = askVO.getAskNum();
		this.userNum = askVO.getUserNum();
		this.categoryNum = askVO.getCategoryNum();
		this.askTitle = askVO.getAskTitle();
		this.askContent = askVO.getAskContent();
		this.askAnswer = askVO.getAskAnswer();
		this.askDate = askVO.getAskDate();
		this.askStatus = askVO.getAskStatus();
	}

	public int getAskNum() {
		return askNum;
	}

	public void setAskNum(int askNum) {
		this.askNum = askNum;
	}

	public int getUserNum() {
		return userNum;
	}

	public void setUserNum(int userNum) {
		this.userNum = userNum;
	}

	public int getCategoryNum() {
		return categoryNum;
	}

	public void setCategoryNum(int categoryNum) {
		this.categoryNum = categoryNum;
	}

	public String getAskTitle() {
		return askTitle;
	}

	public void setAskTitle(String askTitle) {
		this.askTitle = askTitle;
	}

	public String getAskContent() {
		return askContent;
	}

	public void setAskContent(String askContent) {
		this.askContent = askContent;
	}
	
	public String getAskAnswer() {
		return askAnswer;
	}
	
	public void setAskAnswer(String askAnswer) {
		this.askAnswer = askAnswer;
	}


	public String getAskDate() {
		return askDate;
	}

	public void setAskDate(String askDate) {
		this.askDate = askDate;
	}

	public int getAskStatus() {
		return askStatus;
	}

	public void setAskStatus(int askStatus) {
		this.askStatus = askStatus;
	}
	

}
